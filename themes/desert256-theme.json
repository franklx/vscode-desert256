{
    "type": "dark",
    "colors": {
        "editor.background": "#101010",
        "editor.foreground": "#f8f8f8",
        "editorCursor.foreground": "#00ff00",
        "list.highlightForeground": "#e58520",
    },
    "semanticHighlighting": true,
    "tokenColors": [
        { "name": "Comment",
            "scope": "comment",
            "settings": { "foreground": "#87d7ff" }
        },
        { "name": "String",
            "scope": "string",
            "settings": { "foreground": "#ff8700" }
        },
        { "name": "String Embedded Source",
            "scope": "string source",
            "settings": { "foreground": "#ffd787" }
        },
        { "name": "Number",
            "scope": "constant.numeric",
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "Built-in constant",
            "scope": "constant.language",
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other",
            ],
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "Keyword",
            "scope": "keyword",
            "settings": { "foreground": "#f0e770", "fontStyle": "bold" }
        },
        { "name": "Types",
            "scope": [
                "support.type",
                "entity.other.attribute-name",
            ],
            "settings": { "foreground": "#ff5f5f" }
        },
        { "name": "Storage",
            "scope": "storage",
            "settings": { "foreground": "#f0e770", "fontStyle": "bold" }
        },
        { "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": { "foreground": "#ff5f5f" }
        },
        { "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": { "foreground": "#5faf5f" }
        },
        { "name": "Function name",
            "scope": "entity.name.function",
            "settings": { "foreground": "#87ff87" }
        },
        { "name": "Function argument",
            "scope": "variable.parameter",
            "settings": { "foreground": "#ffd787" }
        },
        { "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": { "foreground": "#f0e770", "fontStyle": "bold" }
        },
        { "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": { "foreground": "#d7af87" }
        },

        { "name": "Library function",
            "scope": "support.function",
            "settings": { "foreground": "#5faf5f" }
        },
        { "name": "Function decorator",
            "scope": "meta.function.decorator",
            "settings": { "foreground": "#5faf5f" }
        },
        { "name": "Language Constant",
            "scope": "constant.language",
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "Invalid",
            "scope": "invalid",
            "settings": { "foreground": "#ff0b00" }
        },
        { "name": "Keyword Control",
            "scope": "keyword.control",
            "settings": { "foreground": "#f0e770", "fontStyle": "bold" }
        },
        { "name": "Variable",
            "scope": "variable",
            "settings": { "foreground": "#ffd7d7" }
        },
        { "name": "C/CPP/Haskell Preprocessor",
            "scope": "meta.preprocessor",
            "settings": { "foreground": "#afafff" }
        },
        { "name": "Haskell Function definition",
            "scope": "entity.name.function-definition",
            "settings": { "foreground": "#5faf5f" }
        },
        { "name": "Haskell Type Name",
            "scope": [
                "support.other.module",
                "storage.type.haskell"
            ],
            "settings": { "foreground": "#ff5f5f" }
        },
        { "name": "Haskell Unit",
            "scope": [
                "support.constant.unit.haskell",
                "constant.language.unit.haskell"
            ],
            "settings": { "foreground": "#f0e770" }
        },
        { "name": "Haskell Directive",
            "scope": "keyword.other.preprocessor.haskell",
            "settings": { "foreground": "#afd7ff" }
        },
        { "name": "Tag Name",
            "scope": "entity.name.tag",
            "settings": { "foreground": "#f0e770" }
        },
        { "name": "Doctype",
            "scope": [
                "meta.doctype",
                "meta.tag.sgml-declaration.doctype",
                "meta.tag.sgml.doctype",
            ],
            "settings": { "foreground": "#afafff" }
        },
        { "name": "Tag Attribute",
            "scope": [
                "entity.other.attribute-name",
                "meta.tag punctuation.definition.string",
            ],
            "settings": { "foreground": "#ffd75f" }
        },
        { "name": "Tag Value",
            "scope": [
                "meta.tag string -source -punctuation",
                "text source text meta.tag string -punctuation",
            ],
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "Meta Brace",
            "scope": [
                "punctuation.section.embedded -(source string source punctuation.section.embedded)",
                "meta.brace.erb.html",
            ],
            "settings": { "foreground": "#f0e770" }
        },

        { "name": "Format String Placeholder",
            "scope": "constant.character.format",
            "settings": { "foreground": "#ffd75f" }
        },

        { "name": "HTML String",
            "scope": [
                "string.quoted.double.html",
                "punctuation.definition.string.begin.html",
                "punctuation.definition.string.end.html"
            ],
            "settings": { "foreground": "#ff8700" }
        },
        { "name": "HTML Tags",
            "scope": [
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
            ],
            "settings": { "foreground": "#87ff87" }
        },
        { "name": "Unquoted String",
            "scope": "string.unquoted",
            "settings": { "foreground": "#ff8787" }
        },
        { "name": "CSS Selector",
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css",
            ],
            "settings": { "foreground": "#ffffff" }
        },
        { "name": "CSS ID",
            "scope": "meta.selector.css entity.other.attribute-name.id",
            "settings": { "foreground": "#ffffff" }
        },
        { "name": "CSS Unit",
            "scope": "keyword.other.unit",
            "settings": { "foreground": "#ffaf87" }
        },
        { "name": "CSS Property Name",
            "scope": "support.type.property-name.css",
            "settings": { "foreground": "#ffd787" }
        },

        { "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": { "foreground": "#ff5f5f" }
        },
        { "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": { "foreground": "#ff5f5f" } }
    ],
    "semanticTokenColors": {
      //"namespace":               "#      ", // entity.name.namespace
        "type":                    "#ff5f5f", // entity.name.type
        "type.defaultLibrary":     "#ff5f5f", // support.type
      //"struct":                  "#      ", // storage.type.struct
        "class":                   "#ff5f5f", // entity.name.type.class
      //"class.defaultLibrary":    "#      ", // support.class
      //"interface":               "#      ", // entity.name.type.interface
      //"enum":                    "#      ", // entity.name.type.enum
        "function":                "#87ff87", // entity.name.function
        "function.defaultLibrary": "#5faf5f", // support.function
      //"method":                  "#      ", // entity.name.function.member
      //"macro":                   "#      ", // entity.name.function.macro
      //"variable":                "#      ", // variable.other.readwrite , entity.name.variable
      //"variable.readonly":       "#      ", // variable.other.constant
      //"variable.readonly.defaultLibrary": "#", // support.constant
        "variable.typeHint":       "#ff5f5f",
        "parameter":               "#ffd787", // variable.parameter
      //"property":                "#      ", // variable.other.property
      //"property.readonly":       "#      ", // variable.other.constant.property
      //"enumMember":              "#      ", // variable.other.enummember
      //"event":                   "#      ", // variable.other.event
    }
}
